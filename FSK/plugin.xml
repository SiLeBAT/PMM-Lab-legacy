<extension point="org.knime.workbench.repository.metanode">
  <PersistedMetaNode category-path="/pmmlab" expert-flag="false" id="de.bund.bfr.knime.pmm.fsk.testrmodel" name="Test R model" workflowDir="metanodes_templates/Test R model/" />
</extension>

<extension point="org.knime.workbench.repository.categories">
  <category description="BfR nodes" icon="icons/rsnippet.png" level-id="r" locked="true" name="BfR R nodes" path="/pmmlab" />
  <category after="r-metanodes" icon="icons/rsnippet_io.png" level-id="io" name="IO" path="/pmmlab/r" />
</extension>	
   
</plugin>
