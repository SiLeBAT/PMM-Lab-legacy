  <node after="de.bund.bfr.knime.r.AddTableToRNodeFactory" category-path="/pmmlab/r" factory-class="de.bund.bfr.knime.r.RLearnerNodeFactory" id="de.bund.bfr.knime.r.RLearnerNodeFactory" />
  <node after="de.bund.bfr.knime.r.RLearnerNodeFactory" category-path="/pmmlab/r" factory-class="de.bund.bfr.knime.r.RPredictorNodeFactory" id="de.bund.bfr.knime.r.RPredictorNodeFactory" />
