<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<?eclipse version="3.0"?><plugin>

   <!-- This is an example for providing an own category 
      <extension
         point="org.knime.workbench.repository.categories">
      <category
            description="Category containing my nodes"
            icon="icons/default.png"
            level-id="myNodes"
            name="My nodes"
            path="/"/>
   </extension>
   -->
   
   <extension point="org.knime.workbench.repository.nodes">
   	<node category-path="/pmmlab/reader" factory-class="de.bund.bfr.knime.pmm.sbmlreader.SBMLReaderNodeFactory" id="sbmlreader.SBMLReaderNodeFactory"/>
   	<node category-path="/pmmlab/writer" factory-class="de.bund.bfr.knime.pmm.sbmlwriter.SBMLWriterNodeFactory" id="sbmlwriter.SBMLWriterNodeFactory"/>
<node category-path="/" factory-class="de.bund.bfr.knime.pmm.json.JSONEncoderNodeFactory" id="de.bund.bfr.knime.pmm.jsonencoder.JSONEncoderNodeFactory"/>
</extension>
   
</plugin>
